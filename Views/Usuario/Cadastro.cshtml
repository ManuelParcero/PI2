@model CadUsuarioViewModel
@{
    ViewData["Title"] = "Cadastro de Usuario";
}

<h1 class="row justify-content-md-center">Cadastro de Usuario</h1>
<div class="row justify-content-md-center">
    <form asp-action="Cadastro" method="POST" class="col-md-4">
 <fieldset>
            <legend>Usuario</legend>
            <div class="form-group">
                <label asp-for="Usuario.Nome">Nome</label>
                <input asp-for="Usuario.Nome" class="form-control" readonly="readonly"/>
            </div>
            <div class="form-group">
                <label asp-for="Usuario.Nascimento">Nascimento</label>
                <input asp-for="Usuario.Nascimento" class="form-control" readonly="readonly"/>
            </div>
             <div class="form-group">
                <label asp-for="Usuario.Login">Login</label>
                <input asp-for="Usuario.Login" class="form-control" readonly="readonly"/>
            </div>
             <div class="form-group">
                <label asp-for="Usuario.Email">Email</label>
                <input asp-for="Usuario.Email" class="form-control" readonly="readonly"/>
            </div>
             <div class="form-group">
                <label asp-for="Usuario.senha">senha</label>
                <input asp-for="Usuario.senha" class="form-control" readonly="readonly"/>
            </div>
             <div class="form-group">
                <label asp-for="Usuario.sexo">sexo</label>
                <input asp-for="Usuario.sexo" class="form-control" readonly="readonly"/>
            </div>
             <div class="form-group">
                <label asp-for="Usuario.Inscrito">Inscrito</label>
                <input asp-for="Usuario.Inscrito" class="form-control" readonly="readonly"/>
            </div>
         
        </fieldset>
        <input type="submit" value="Salvar" class="btn btn-primary"/>
    </form>
</div>